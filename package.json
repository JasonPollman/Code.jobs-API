  "scripts": {
    "compile": "NODE_ENV=production ./node_modules/.bin/babel src -d dist --copy-files",
    "lint": "./node_modules/.bin/eslint ./src",
    "start-dev": "NODE_ENV=development ./node_modules/.bin/babel-node ./src/bin/api.js | bunyan --output short --time local",
    "start": "NODE_ENV=production npm run compile && node ./dist/bin/api.js",
