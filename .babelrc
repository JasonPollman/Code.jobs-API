{
  "sourceMaps": "inline",
  "retainLines": true,
  "comments": true,
  "compact": "auto",
  "minified": false,
  "presets": [
    "latest"
  ],
  "plugins": [
    "source-map-support-for-6",
    "transform-class-properties",
    "transform-promise-to-bluebird",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-runtime",
    "add-module-exports"
  ],
  "env": {
    "production": {
      "retainLines": false,
      "comments": false,
      "compact": true,
      "minified": true
    },
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}